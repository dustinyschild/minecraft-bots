{
  "name": "node-minecraft-bot",
  "version": "0.0.0",
  "description": "A bot for automating minecraft tasks",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "scripts": {
    "build": "ts-standard && tsc",
    "start": "tsc && node ./dist/index.js",
    "statemachine": "tsc && node ./dist/BotBase/index.js",
    "sorter": "tsc && node ./dist/SortingBot/index.js",
    "test:multiple": "tsc && node ./dist/testMultiple.js"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "mineflayer": "^4.8.1",
    "mineflayer-collectblock": "^1.4.1",
    "mineflayer-pathfinder": "^2.4.2",
    "mineflayer-statemachine": "^1.7.0",
    "mineflayer-web-inventory": "^1.8.4",
    "prismarine-block": "^1.16.3",
    "prismarine-entity": "^2.2.0",
    "prismarine-item": "^1.12.1",
    "prismarine-viewer": "^1.24.0",
    "ts-standard": "^12.0.2",
    "tslib": "~2.4",
    "vec3": "^0.1.8"
  },
  "devDependencies": {
    "@types/jest": "~29.2",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.3",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tsutils": "~3.21",
    "typescript": "~4.9"
  },
  "author": "Jakub Synowiec <jsynowiec@users.noreply.github.com>",
  "license": "Apache-2.0",
  "volta": {
    "node": "18.12.1"
  }
}
